# Production environment variables
VITE_API_URL=https://api.vms.example.com/api/v1
VITE_APP_NAME=VMS
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBHOOKS=true
VITE_ENABLE_GDPR=true

# External services
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_GOOGLE_ANALYTICS_ID=GA-PRODUCTION-ID

# Upload limits
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true